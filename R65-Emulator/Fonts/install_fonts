# install_DSEG - install DSEG fonts

mkdir /usr/share/fonts/truetype/DSEG7
cp DSEG7*.ttf /usr/share/fonts/truetype/DSEG7

mkdir /usr/share/fonts/truetype/DSEG14
cp DSEG14*.ttf /usr/share/fonts/truetype/DSEG14

fc-cache /usr/share/fonts

fc-list "DSEG7 Classic"
fc-list "DSEG7 Classic-Bold"
fc-list "DSEG14 Classic"
fc-list "DSEG14 Classic-Bold"
